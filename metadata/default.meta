
# Application-level permissions

[]
access = read : [ * ], write : [ admin, sc_admin ]
export = system

# The following elements define the views given by the app to the users.

[views/readme]
owner = nobody
access = read : [ * ], write : [ admin, sc_admin ]

# The following elements define how the lookups are exportable.

[lookups/AbuseIPDB_Categories.csv]
access = read : [ * ], write : [ admin, sc_admin ]
export = system

[transforms/AbuseIPDB_Categories]
access = read : [ * ], write : [ admin, sc_admin ]
export = system

# The following elements need to be exported for the AbuseIPDB command to work outside the app

[commands/abuseipdb]
export = system
access = read : [ admin, sc_admin ], write : [ admin, sc_admin ]

[abuseipdb]
export = system
access = read : [ admin, sc_admin ], write : [ admin, sc_admin ]

[searchbnf/abuseipdb-command]
export = system
access = read : [ * ], write : [ admin, sc_admin ]

[searchbnf/abuseipdb-options]
export = system
access = read : [ * ], write : [ admin, sc_admin ]

# For accessing the API key in all Splunk apps.

[passwords]
access = read : [ * ], write : [ admin, sc_admin ]
export = system

# For enabling alert actions on all applications.

[alert_actions]
export = system
access = read : [ * ], write : [ admin, sc_admin ]

# For accessing the dashboards only in the app.

[views/abuseipdb__alert_action]
owner = nobody
export = none
access = read : [ * ], write : [ admin, sc_admin ]